{
  "manifest_version": 3,
  "name": "Device Preview Extension",
  "version": "1.0.0",
  "description": "View websites in multiple device viewports simultaneously",
  "permissions": ["activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Device Preview"
  },
  "icons": {
    "16": "logo1.png",
    "48": "logo1.png",
    "128": "logo2.png"
  }
}
